    {
      "name": "Post",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "// Parse the response body to JSON\r",
              "var responseJson = pm.response.json();\r",
              "\r",
              "// Capture the individual details and store them directly in collection variables\r",
              "pm.collectionVariables.set(\"id\", responseJson.id);    // Store 'id' in collection variables\r",
              "pm.collectionVariables.set(\"Name\", responseJson.Name); // Store 'Name' in collection variables\r",
              "\r",
              "// Log the captured values for verification\r",
              "console.log(\"Captured customer details:\");\r",
              "console.log(pm.collectionVariables.get(\"id\"));\r",
              "console.log(pm.collectionVariables.get(\"Name\"));\r",
              "\r",
              "// Test for the correct Customer ID format\r",
              "pm.test(\"Customer ID has a valid format\", function () {\r",
              "    pm.expect(responseJson.id).to.be.a('number');\r",
              "});\r",
              "// Validate Customer ID/Number\r",
              "pm.test(\"Customer ID is correct\", function () {\r",
              "    pm.expect(pm.response.json().id.toString()).to.eql(\"101\");\r",
              "});\r",
              "// validate Customer Name\r",
              "pm.test(\"Customer Name is correct\", function () {\r",
              "    pm.expect(pm.response.json().Name).to.eql(\"Abc\");\r",
              "});\r",
              "\r",
              ""
            ],
            "type": "text/javascript",
            "packages": {}
          }
        }
